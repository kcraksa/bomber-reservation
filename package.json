{
  "name": "bomber-web-reservation",
  "private": true,
  "version": "1.0.0",
  "description": "Bomber web reservation",
  "repository": {
    "type": "git",
    "url": "https://github.com/bomber-org/bomber-web-reservation"
  },
  "keywords": [
    "vite",
    "react",
    "starter",
    "template"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --emptyOutDir",
    "serve": "vite preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "check:lint": "eslint --ignore-path .gitignore . --ext .js,.jsx,.ts,.tsx",
    "check:prettier": "prettier --config .prettierrc ./src/ --check",
    "check:ts": "tsc",
    "checker": "yarn check:lint && yarn tsc && yarn jest",
    "format:lint": "eslint --fix ./src/ --ext .js,.jsx,.ts,.tsx",
    "format:prettier": "prettier --config .prettierrc ./src/ --write",
    "formatter": "yarn format:lint && yarn format:prettier",
    "lint:eslint": "eslint . --ext .js,.jsx",
    "fix:eslint": "eslint --fix  . --ext .js,.jsx",
    "lint:stylelint": "stylelint **/*.{css,scss,jsx}",
    "fix:stylelint": "stylelint --fix  ./src/**/*.{css, scss,jsx}",
    "test": "jest",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "and": "^0.0.3",
    "axios": "^1.6.8",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "flowbite": "^2.3.0",
    "flowbite-datepicker": "^1.2.6",
    "flowbite-react": "^0.7.6",
    "formik": "^2.4.5",
    "i18next": "^23.10.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-calendar": "^4.8.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^14.1.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.22.3",
    "rsuite": "^5.59.0",
    "sass": "^1.54.5",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@emotion/babel-plugin": "^11.10.2",
    "@emotion/babel-preset-css-prop": "^11.10.0",
    "@namics/stylelint-bem": "^7.0.0",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@vitejs/plugin-react": "^2.0.1",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.0.1",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^9.0.11",
    "jest": "^29.0.1",
    "postcss": "^8.4.16",
    "postcss-scss": "^4.0.4",
    "postcss-syntax": "^0.36.2",
    "prettier": "^3.2.5",
    "prettier-plugin-style-order": "^0.2.2",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "react-test-renderer": "^18.2.0",
    "stylelint": "^14.11.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.2",
    "vite": "^3.0.9",
    "webpack": "^5.74.0"
  }
}
